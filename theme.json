					"name": "Brand Dark",
					"slug": "duotone-5"
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": true,
			"customSpacingSize": true,
			"margin": true,
			"padding": true,
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Natural",
					"slug": "natural",
					"shadow": "0 4px 8px -2px #000d"
				},
				{
					"name": "Soft",
					"slug": "soft",
					"shadow": "0 2px 1px #0002, 0 4px 2px #0002, 0 8px 4px #0002, 0 16px 8px #0002, 0 32px 16px #0001"
				},
				{
					"name": "Tight",
					"slug": "tight",
					"shadow": "0 2px 3px #0002, 0 2px 2px #0004"
				},
				{
					"name": "Crisp",
					"slug": "crisp",
					"shadow": "6px 6px 0 #0004"
				},
				{
					"name": "Outline",
					"slug": "outline",
					"shadow": "10px 10px 0 -3px var(--wp--preset--color--base), 10px 10px 0 0 #000"
				},
				{
					"name": "Fancy",
					"slug": "fancy",
					"shadow": "-9px -9px 0 0 var(--wp--preset--color--primary)"
				},
				{
					"name": "Steps",
					"slug": "steps",
					"shadow": "3px 3px 0 0 #00000059,6px 6px 0 0 #0000004d, 9px 9px 0 0#00000033, 12px 12px 0 0 #0000001a, 16px 16px 0 0 #0000000d"
				},
				{
					"name": "Seventies",
					"slug": "seventies",
					"shadow": "2px 2px 0 0 #F6AF09,4px 4px 0 0 #F37718,6px 6px 0 0 #DB3922,8px 8px 0 0 #402419"
				},				
				{
					"name": "Inset",
					"slug": "inset",
					"shadow": "inset 6px 6px 11px -1px #00000033,inset 3px 3px 3px -1px #00000066"
				}
			]
		},
		"typography": {
			"fluid": true,
			"customFontSize": true,
			"dropCap": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textColumns": true,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": true,
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "var(--wp--custom--secondary-font), system-ui, -apple-system, 'Segoe UI', Roboto, 'Liberation Sans', sans-serif",
					"name": "Secondary Font",
					"slug": "secondary-font"
				},
				{
					"fontFamily": "var(--wp--custom--primary-font), system-ui, -apple-system, 'Segoe UI', Roboto, 'Liberation Sans', sans-serif",
					"name": "System Sans",
					"slug": "system-font"
				},
				{
					"fontFamily": "Georgia, 'Droid Serif', Times, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
					"name": "System Serif",
					"slug": "system-serif"
				},
				{
					"fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, 'Roboto Mono', 'Liberation Mono', monospace",
					"name": "System Mono",
					"slug": "system-mono"
				},
				{
					"fontFamily": "'Brush Script MT', cursive",
					"name": "System Cursive",
					"slug": "system-cursive"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.8rem",
						"max": "1rem"
					},
					"size": "1rem",
					"name": "Small",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.2rem"
					},
					"size": "1.2rem",
					"name": "Medium",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "1.7rem"
					},
					"size": "1.6rem",
					"name": "Large",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.6rem"
					},
					"size": "2.3rem",
					"name": "Extra Large",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.5rem",
						"max": "3.6rem"
					},
					"size": "3.6rem",
					"name": "X-Extra Large",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"css": "span[style*='underline'],u{text-decoration-thickness:3px;text-decoration-color:var(--wp--preset--color--secondary);}code,kbd,samp,pre{font-family:var(--wp--preset--font-family--system-mono);unicode-bidi:bidi-override;white-space:pre-wrap;font-size:inherit}iframe{border:0}b,strong{font-weight:bolder}sub,sup{font-size:75%;line-height:0;position:relative}kbd{background-color:#ddd;padding:1px 4px;border-radius:5px;border:1px solid #aaa;border-bottom-width:3px;white-space:nowrap}.has-text-align-center{text-wrap:balance}a{text-decoration-thickness:1px!important;text-underline-offset:.1em}.is-root-container{max-width:var(--wp--style--global--content-size);margin:1em auto}.wp-block-gallery:not(.is-style-default) figure.wp-block-image:has(figcaption):before{backdrop-filter:none;-webkit-mask-image:none;mask-image:none}.wp-block-gallery:not(.is-style-default)",
		"color": {
			"background": "var:preset|color|base",
			"text": "var:preset|color|contrast"
		},
		"elements": {
			"h1": {
				"css": "background-image:linear-gradient(0deg,#e65100 0%,#f4971c 54%,#ffe700 100%);-webkit-background-clip:text;background-clip:text;-webkit-text-fill-color:transparent;filter:drop-shadow(1px 1px 0 #444) drop-shadow(1px 1px 0px #777)",
				"color": {
					"text": "var:preset|color|secondary"
				},
				"typography": {
					"fontSize": "var:preset|font-size|xx-large",
					"fontWeight": "700",
					"letterSpacing": "-0.1rem"
				},
				"spacing": {
					"padding": {
						"bottom": "4px"
					}
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2.2rem",
					"fontWeight": "600"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.75rem",
					"fontWeight": "600"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.6rem",
					"fontWeight": "600"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.25rem",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1.1rem",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"lineHeight": "1.15"
				},
				"spacing": {
					"margin": {
						"bottom": "0.5rem"
					}
				}
			},
			"caption": {
				"typography": {
					"fontSize": "0.9rem",
					"lineHeight": "1.15"
				},
				"spacing": {
					"padding": {
						"top": "0.3em"
					}
				}
			},
			"cite": {
				"typography": {
					"lineHeight": "1.15"
				}
			},
			"button": {
				"css": "transition:all 0.25s ease-in-out",
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var:preset|color|primary",
					"text": "var:preset|color|black"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|primary-pale",
						"text": "var:preset|color|black"
					}
				},
				":active": {
					"color": {
						"background": "var:preset|color|primary-dark",
						"text": "var:preset|color|white"
					}
				},
				":focus": {
					"color": {
						"background": "var:preset|color|black",
						"text": "var:preset|color|white"
					}
				}
			},
			"link": {
				":link": {
					"color": {
						"text": "inherit"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"right": "var:preset|spacing|30",
				"bottom": "var:preset|spacing|40",
				"left": "var:preset|spacing|30",
				"top": "var:preset|spacing|40"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|system-font",
			"lineHeight": "1.7",
			"fontSize": "var:preset|font-size|medium"
		},
		"blocks": {
			"core/paragraph": {
				"css": "text-wrap:pretty;orphans:3;widows:3; &.has-drop-cap:not(:focus)::first-letter{font-weight:700 !important;font-size:450% !important} & code{background-color:#aaa6;padding:0 2px;border-radius:2px} & s{-webkit-text-decoration:wavy line-through red;text-decoration:wavy line-through red;text-decoration-thickness:10%;}",
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "1.7"
				},
				"spacing": {
					"margin": {
						"bottom": "0.75rem"
					}
				},
				"variations": {
					"text-shadow-soft": {
						"css": "text-shadow:.1rem .1rem .1rem #0009,.2rem .2rem .3rem #0003"
					},
					"text-shadow-hard": {
						"css": "text-shadow:.08rem .08rem 0 #000a"
					},
					"text-shadow-glow": {
						"css": "text-shadow:0 0 0.3125rem #fff,0 0 0.625rem #fff,0 0 0.9375rem #fff,0 0 1.25rem #FF2D95,0 0 1.875rem #FF2D95,0 0 2.5rem #FF2D95,0 0 3.125rem #FF2D95,0 0 4.6875rem #FF2D95,0.125rem 0.125rem 0.125rem #0000;"
					},
					"text-outline": {
						"css": "stroke:#222;stroke-width:2px;-webkit-text-stroke-color:#222;-webkit-text-stroke-width:2px;paint-order:stroke fill"
					},
					"text-justify": {
						"typography": {
							"textAlign": "justify"
						}
					},
					"text-subtitle": {
						"color": {
							"text": "#555"
						},
						"typography": {
							"fontSize": "1.5rem",
							"fontWeight": "600",
							"lineHeight": "1.1 !important",
							"letterSpacing": "-1px",
							"fontFamily": "var:preset|font-family|secondary-font"
						},
						"spacing": {
							"margin": {
								"top": "0.6em",
								"bottom": "0.6em",
								"left": "0.1em",
								"right": "0.1em"
							}
						}
					}
				}
			},
			"core/heading": {
				"css": "margin:1.2rem auto 0.5rem;text-wrap:pretty",
				"typography": {
					"lineHeight": "1"
				},
				"variations": {
					"header-shadow-soft": {
						"css": "text-shadow:.1rem .1rem 2px #000a,.2rem .2rem 7px #0004"
					},
					"header-shadow-hard": {
						"css": "text-shadow:.1rem .1rem 1px #000a"
					},
					"header-shadow-3d": {
						"css": "text-shadow:-1px 1px 0 #666,-2px 2px 0 #555,-3px 3px 0 #444,-4px 4px 0 #333,-5px 5px 4px #0008"
					},
					"header-shadow-inset": {
						"css": "text-shadow:1px 1px 1px #fff8,0 0 2px #fff7"
					},
					"header-text-fill": {
						"css": "padding:0 !important;-webkit-background-clip:text !important;background-clip:text !important;-webkit-text-fill-color:transparent;line-height:1.1"
					},
					"header-embelish": {
						"css": "&::before {content:''; width:80px; height:80px; margin:-20px -10px -50px -10px; display:block; background-color:var(--wp--preset--color--primary-pale); box-shadow:-4px -4px 0 0 var(--wp--preset--color--primary-dark)} &:is(.has-text-align-center)::before {margin-left:calc(var(--wp--style--global--content-size) / 2 - 30px)}"
