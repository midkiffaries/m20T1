				{
					"gradient": "linear-gradient(0deg, #ffffff 2%, #efccfc 7%, #de00ff 20%, #5400c1 50%, #e8f9ff 51%, #00d4ff 66%, #0086d6 80%, #0065a2 93%)",
					"name": "Chrome gradient",
					"slug": "chromegradient"
