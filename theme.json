					"gradient": "linear-gradient(0deg,#ffd000 0%,#ffd000 20%,#f6af09 20%,#f6af09 40%,#f37c17 40%,#f37718 60%,#dd3f21 60%,#db3922 80%,#4d261a 80%,#402419 100%)",
