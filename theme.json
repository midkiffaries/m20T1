>\")} &:is(.has-text-align-center)::before {margin-left:calc(var(--wp--style--global--content-size) / 2 - 30px)}"
					},
					"header-outline": {
						"css": "color:var(--wp--preset--color--primary);text-shadow:1px 0 #222,1px 1px #222,-3px 3px #222,-1px -1px #222,1px -1px #222,-1px 1px #222,1px 1px #222"
					},
					"header-stripes": {
						"css": "display:flex; &::before {background-repeat:space repeat; background-size:1em; content:''; flex-grow:1} &::after {background-repeat:space repeat; background-size:1em; content:''; flex-grow:1} &:not(.has-text-align-right, .has-text-align-center)::before {flex-grow:0} &:is(.has-text-align-right)::after {flex-grow:0}"
					}
				}
			},
			"core/table": {
				"css": "border-collapse:collapse;border-spacing:0;text-indent:0;max-width:100%;font-size:1rem; & thead {display:table-header-group} & td,th{padding:0.15em 0.5em;line-height:1.15;text-align:inherit;text-overflow:ellipsis;vertical-align:text-top;font-variant-numeric:tabular-nums}",
				"variations": {
					"table-theme": {
						"css": "& thead,tfoot {background-color:var(--wp--preset--color--primary)} & td {background-color:#fff1} & tbody tr:nth-child(even) {background-color:#ccc5}"
					}
				}
