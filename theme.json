{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"description": "https://developer.wordpress.org/themes/global-settings-and-styles/introduction-to-theme-json/",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": true,
			"defaultGradients": false,
			"defaultPalette": true,
			"link": true,
			"text": true,
			"palette": [
				{
					"color": "#40A5FF",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#205380",
					"name": "Primary (Dark)",
					"slug": "primaryDark"
				},
				{
					"color": "#8CC9FF",
					"name": "Primary (Pale)",
					"slug": "primaryPale"
				},
				{
					"color": "#e65100",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#ffd726",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#bada55",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			],
			"gradients": [
				{
					"gradient": "repeating-linear-gradient(135deg, rgba(0,0,0,0.03) 0px, rgba(0,0,0,0.03) 2px,transparent 2px, transparent 4px), linear-gradient(90deg, #83bbf7, #3b68f7)",
					"name": "A gradient with the brand colors",
					"slug": "brandGradient"
				},
				{
					"gradient": "linear-gradient(0deg, #205380, #03000f)",
					"name": "A dark brand color gradient",
					"slug": "branddarkGradient"
				},
				{
					"gradient": "linear-gradient(135deg, #697e9b, #bbcce8)",
					"name": "A blueish grey gradient",
					"slug": "greyGradient"
				},
				{
					"gradient": "linear-gradient(0deg, #03000f, #596c75)",
					"name": "A dark gradient",
					"slug": "darkGradient"
				}
			]
		},
		"custom": {},
		"layout": {
			"contentSize": "1060px",
			"wideSize": "1200px"
		},
		"dimensions": {
			"minHeight": true
		},
		"spacing": {
			"spacingScale": {
				"steps": 1
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "5"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "6"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"position": {
			"sticky": true
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Natural",
					"slug": "natural",
					"shadow": "0 4px 8px -2px rgba(0,0,0,0.8)"
				},
				{
					"name": "Soft",
					"slug": "soft",
					"shadow": "0 2px 1px rgba(0,0,0,0.1), 0 4px 2px rgba(0,0,0,0.1), 0 8px 4px rgba(0,0,0,0.1), 0 16px 8px rgba(0,0,0,0.1), 0 32px 16px rgba(0,0,0,0.09)"
				},
				{
					"name": "Tight",
					"slug": "tight",
					"shadow": "0 1px 3px rgba(0,0,0,0.12), 0 1px 2px rgba(0,0,0,0.24)"
				},
				{
					"name": "Crisp",
					"slug": "crisp",
					"shadow": "6px 6px 0 rgba(0,0,0,0.3)"
				},
				{
					"name": "Fancy",
					"slug": "fancy",
					"shadow": "0 0 0 1px #fff, 7px 7px 0 0 #888, -8px -8px 0 0 var(--wp--preset--color--primary)"
				},
				{
					"name": "Raised",
					"slug": "raised",
					"shadow": "0 2px 4px rgba(0,0,0,0.4), 0 7px 13px -3px rgba(0,0,0,0.3), inset 0 -3px 0 rgba(0,0,0,0.2)"
				},
				{
					"name": "Inset",
					"slug": "inset",
					"shadow": "inset 2px 3px 7px -1px rgba(0,0,0,0.5)"
				}
			]
		},
		"typography": {
			"fluid": true,
			"customFontSize": true,
			"dropCap": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textColumns": true,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": true,
			"fontFamilies": [
				{
					"fontFamily": "system-ui, -apple-system, Segoe UI, Roboto, Liberation Sans, sans-serif",
					"name": "System Sans",
					"slug": "system-font"
				},
				{
					"fontFamily": "Georgia, Droid Serif, Times, serif",
					"name": "System Serif",
					"slug": "system-serif"
				},
				{
					"fontFamily": "cursive",
					"name": "System Cursive",
					"slug": "system-cursive"
				},
				{
					"fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, Roboto Mono, Liberation Mono, monospace",
					"name": "System Mono",
					"slug": "system-mono"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.8rem",
						"max": "1rem"
					},
					"size": "1rem",
					"name": "Small",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.2rem"
					},
					"size": "1.2rem",
					"name": "Medium",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.6rem",
						"max": "1.8rem"
					},
					"size": "1.6rem",
					"name": "Large",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2.2rem",
					"name": "Extra Large",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "4rem",
						"max": "5rem"
					},
					"size": "4rem",
					"name": "Extra Extra Large",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true,
		"blocks": {}
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"bottom": "1.25rem",
						"top": "1.25rem"
					}
				},
				"typography": {
					"fontWeight": "400"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--secondary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comments-title":{
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/comment-author-name": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--secondary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--secondary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--40)"
					}
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/pullquote": {
				"border": {
					"style": "solid",
					"width": "1px 0"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal",
							"textTransform": "none"
						}
					}
				},
				"typography": {
					"lineHeight": "1.3"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40) !important",
						"top": "var(--wp--preset--spacing--40) !important"
					}
				}
			},
			"core/query": {
				"elements": {
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)"
						}
					}
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--secondary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "normal",
					"lineHeight": "1.4"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--black)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--black)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--black)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--black)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "2.625rem",
					"lineHeight": "1.15",
					"fontWeight": "700"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2.2rem",
					"lineHeight": "1.15",
					"fontWeight": "600"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.75rem",
					"lineHeight": "1.15",
					"fontWeight": "600"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.6rem",
					"lineHeight": "1.15",
					"fontWeight": "600"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.25rem",
					"lineHeight": "1.15",
					"fontWeight": "600",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1.1rem",
					"lineHeight": "1.15",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "1.15"
				}
			},
			"caption": {
				"typography": {
					"fontSize": "0.9rem",
					"textAlign": "center",
					"lineHeight": "1.15"
				}
			},
			"cite": {
				"typography": {
					"fontSize": "smaller",
					"lineHeight": "1.15"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"top": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--30)",
				"bottom": "var(--wp--preset--spacing--40)",
				"left": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"XfontFamily": "var(--wp--preset--font-family--system-font)",
			"lineHeight": "1.15"
		}
	},
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": ["page", "post"],
			"title": "Blank"
		},
		{
			"name": "blog-alternative",
			"postTypes": ["page"],
			"title": "Blog (Alternative)"
		},
		{
			"name": "404",
			"postTypes": ["page"],
			"title": "404"
		}
	],
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		},
		{
			"area": "uncategorized",
			"name": "post-meta",
			"title": "Post Meta"
		},
		{
			"area": "uncategorized",
			"name": "sidebar",
			"title": "Sidebar"
		}
	],
	"patterns": []
}