{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"description": "https://developer.wordpress.org/themes/global-settings-and-styles/introduction-to-theme-json/",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"custom": {
			"primaryFont": "'Montserrat'",
			"secondaryFont": "'Permanent Marker'"
		},
		"layout": {
			"contentSize": "1060px",
			"wideSize": "1250px"
		},
		"dimensions": {
			"minHeight": true,
			"aspectRatio": true
		},
		"position": {
			"sticky": true
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"link": true,
			"text": true,
			"background": true,
			"button": true,
			"heading": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultPalette": true,
			"defaultDuotone": false,
			"defaultGradients": true,
			"palette": [
				{
					"color": "#40A5FF",
					"name": "Brand Primary",
					"slug": "primary"
				},
				{
					"color": "#205380",
					"name": "Brand Dark",
					"slug": "primaryDark"
				},
				{
					"color": "#8CC9FF",
					"name": "Brand Pale",
					"slug": "primaryPale"
				},
				{
					"color": "#e65100",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#ffd726",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#bada55",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(90deg, #40a5ff, #69b7ff, #8cc9ff)",
					"name": "Gradient with the brand colors",
					"slug": "brandgradient"
				},
				{
					"gradient": "linear-gradient(0deg, #40A5FF, #23558a, #021125)",
					"name": "Brand color and black gradient",
					"slug": "brandblackgradient"
				},
				{
					"gradient": "linear-gradient(0deg, #205380, #202a44, #03000f)",
					"name": "Dark brand color gradient",
					"slug": "branddarkgradient"
				},
				{
					"gradient": "linear-gradient(0deg, #e65100, #eb8e00, #e6c400)",
					"name": "Secondary color gradient",
					"slug": "secondarygradient"
				},
				{
					"gradient": "linear-gradient(0deg, #697e9b, #91a4c1, #bbcce8)",
					"name": "Blueish grey gradient",
					"slug": "greygradient"
				},
				{
					"gradient": "linear-gradient(0deg, #d8daed, #dfe9f3, #f0f5f7)",
					"name": "Light grey gradient",
					"slug": "lightgradient"
				},
				{
					"gradient": "repeating-linear-gradient(135deg, rgba(0,0,0,0.03) 0px, rgba(0,0,0,0.03) 2px,transparent 2px, transparent 4px), linear-gradient(90deg, #83bbf7, #4a95fc, #3b68f7)",
					"name": "Custom gradient with the brand colors and strips",
					"slug": "customgradient"
				}
			],
			"duotone": [
				{
					"colors": ["#111111", "#ffffff"],
					"name": "Monochrome",
					"slug": "duotone-1"
				},
				{
					"colors": ["#36200C", "#DCC0A6"],
					"name": "Sepia",
					"slug": "duotone-2"
				},
				{
					"colors": ["#E3DFDF", "#4F2E24"],
					"name": "Negative",
					"slug": "duotone-3"
				},
				{
					"colors": ["#205380", "#8CC9FF"],
					"name": "Brand Light",
					"slug": "duotone-4"
				},
				{
					"colors": ["#000000", "#40A5FF"],
					"name": "Brand Dark",
					"slug": "duotone-5"
				}
			]
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "1",
					"slug": "10",
					"size": "1rem"
				},
				{
					"name": "2",
					"slug": "20",
					"size": "min(1.5rem, 2vw)"
				},
				{
					"name": "3",
					"slug": "30",
					"size": "min(2.5rem, 3vw)"
				},
				{
					"name": "4",
					"slug": "40",
					"size": "min(4rem, 5vw)"
				},
				{
					"name": "5",
					"slug": "50",
					"size": "min(6.5rem, 8vw)"
				},
				{
					"name": "6",
					"slug": "60",
					"size": "min(10.5rem, 13vw)"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Natural",
					"slug": "natural",
					"shadow": "0 4px 8px -2px rgba(0,0,0,0.8)"
				},
				{
					"name": "Soft",
					"slug": "soft",
					"shadow": "0 2px 1px rgba(0,0,0,0.1), 0 4px 2px rgba(0,0,0,0.1), 0 8px 4px rgba(0,0,0,0.1), 0 16px 8px rgba(0,0,0,0.1), 0 32px 16px rgba(0,0,0,0.09)"
				},
				{
					"name": "Tight",
					"slug": "tight",
					"shadow": "0 1px 3px rgba(0,0,0,0.12), 0 1px 2px rgba(0,0,0,0.24)"
				},
				{
					"name": "Crisp",
					"slug": "crisp",
					"shadow": "6px 6px 0 rgba(0,0,0,0.3)"
				},
				{
					"name": "Fancy",
					"slug": "fancy",
					"shadow": "0 0 0 1px #fff, 7px 7px 0 0 #888, -8px -8px 0 0 var(--wp--preset--color--primary)"
				},
				{
					"name": "Raised",
					"slug": "raised",
					"shadow": "0 2px 4px rgba(0,0,0,0.4), 0 7px 13px -3px rgba(0,0,0,0.3), inset 0 -3px 0 rgba(0,0,0,0.2)"
				},
				{
					"name": "Inset",
					"slug": "inset",
					"shadow": "inset 2px 3px 7px -1px rgba(0,0,0,0.5)"
				}
			]
		},
		"typography": {
			"fluid": true,
			"customFontSize": true,
			"dropCap": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textColumns": true,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": true,
			"fontFamilies": [
				{
					"fontFamily": "var(--wp--custom--secondary-font), system-ui, -apple-system, 'Segoe UI', Roboto, 'Liberation Sans', sans-serif",
					"name": "Secondary Font",
					"slug": "secondary-font"
				},
				{
					"fontFamily": "var(--wp--custom--primary-font), system-ui, -apple-system, 'Segoe UI', Roboto, 'Liberation Sans', sans-serif",
					"name": "System Sans",
					"slug": "system-font"
				},
				{
					"fontFamily": "Georgia, 'Droid Serif', Times, serif",
					"name": "System Serif",
					"slug": "system-serif"
				},
				{
					"fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, 'Roboto Mono', 'Liberation Mono', monospace",
					"name": "System Mono",
					"slug": "system-mono"
				},
				{
					"fontFamily": "'Brush Script MT', cursive",
					"name": "System Cursive",
					"slug": "system-cursive"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.8rem",
						"max": "1rem"
					},
					"size": "1rem",
					"name": "Small",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.2rem"
					},
					"size": "1.2rem",
					"name": "Medium",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.6rem",
						"max": "1.8rem"
					},
					"size": "1.6rem",
					"name": "Large",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2.2rem",
					"name": "Extra Large",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "4rem",
						"max": "5rem"
					},
					"size": "4rem",
					"name": "Extra Extra Large",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"css": ".alignjustify{text-align:justify} p.has-drop-cap:not(:focus):first-letter{font-weight:700;font-size:5rem} .subtitle {font-size:1.5rem;margin:0.4em 0.2em 0 0;padding:0;line-height:1.15}",
		"color": {
			"background": "#fdfdfd",
			"text": "#111"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "2.625rem",
					"fontWeight": "700"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2.2rem",
					"fontWeight": "600"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.75rem",
					"fontWeight": "600"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.6rem",
					"fontWeight": "600"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.25rem",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1.1rem",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"lineHeight": "1.15"
				},
				"spacing": {
					"margin": {
						"bottom": "0.5rem"
					}
				}
			},
			"caption": {
				"typography": {
					"fontSize": "0.9rem",
					"lineHeight": "1.15"
				},
				"spacing": {
					"padding": {
						"top": "0.3em"
					}
				}
			},
			"cite": {
				"typography": {
					"lineHeight": "1.15"
				}
			},
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--black)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary-pale)",
						"text": "var(--wp--preset--color--black)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--primary-dark)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--black)",
						"text": "var(--wp--preset--color--white)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"top": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--30)",
				"bottom": "var(--wp--preset--spacing--40)",
				"left": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"lineHeight": "1.7",
			"fontSize": "var(--wp--preset--font-size--medium)"
		},
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"spacing": {
					"margin": {
						"bottom": "0.75rem"
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/pullquote": {
				"border": {
					"style": "solid",
					"width": "1px 0"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				},
				"typography": {
					"lineHeight": "1.2"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--20)",
						"top": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--20)",
						"right": "var(--wp--preset--spacing--20)"
					}
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": ["page", "post"],
			"title": "Blank"
		},
		{
			"name": "blog-alternative",
			"postTypes": ["page"],
			"title": "Blog (Alternative)"
		},
		{
			"name": "404",
			"postTypes": ["page"],
			"title": "404"
		}
	],
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"patterns": []
}