{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"description": "https://developer.wordpress.org/themes/global-settings-and-styles/introduction-to-theme-json/",
	"version": 3,
	"title": "M20T1 Base",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"custom": {
			"sidebarSize": "15em",
			"headerHeight": "15em",
			"headerMobileHeight": "14em",
			"footerColor": "#212121",
			"footerBackground": "linear-gradient(135deg,#eaeaea0f 0%,#eaeaea0f 50%,#a9a9a90f 50%,#a9a9a90f 100%),linear-gradient(90deg,#141414,#141414)",
			"primaryFont": "'Montserrat'",
			"secondaryFont": "'Permanent Marker'"
		},
		"layout": {
			"contentSize": "1060px",
			"wideSize": "1250px"
		},
		"dimensions": {
			"minHeight": true,
			"aspectRatio": true,
			"defaultAspectRatios": true,
			"aspectRatios": []
		},
		"position": {
			"sticky": false
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"link": true,
			"text": true,
			"background": true,
			"button": true,
			"heading": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultPalette": true,
			"defaultDuotone": false,
			"defaultGradients": true,
			"palette": [
				{
					"color": "#40a5ff",
					"name": "Brand Primary",
					"slug": "primary"
				},
				{
					"color": "#205380",
					"name": "Brand Dark",
					"slug": "primaryDark"
				},
				{
					"color": "#66b7ff",
					"name": "Brand Pale",
					"slug": "primaryPale"
				},
				{
					"color": "#e65100",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#ffd726",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#bada55",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#fdfdfd",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#111111",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#cec7c7",
					"name": "Warm",
					"slug": "warm"
				},
				{
					"color": "#37a1ffaa",
					"name": "Highlight",
					"slug": "highlight"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(0deg, #40a5ff, #69b7ff, #8cc9ff)",
					"name": "Brand color gradient",
					"slug": "brandgradient"
				},
				{
					"gradient": "linear-gradient(0deg, #40A5FF, #23558a, #021125)",
					"name": "Brand color and black gradient",
					"slug": "brandblackgradient"
				},
				{
					"gradient": "linear-gradient(0deg, #205380, #202a44, #03000f)",
					"name": "Dark brand color gradient",
					"slug": "branddarkgradient"
				},
				{
					"gradient": "linear-gradient(0deg, #e65100 0%, #f4971c 54%, #ffe700 100%)",
					"name": "Secondary color gradient",
					"slug": "secondarygradient"
				},
				{
					"gradient": "linear-gradient(0deg, #697e9b, #91a4c1, #bbcce8)",
					"name": "Blueish grey gradient",
					"slug": "greygradient"
				},
				{
					"gradient": "linear-gradient(0deg, #d8daed, #dfe9f3, #f0f5f7)",
					"name": "Light grey gradient",
					"slug": "lightgradient"
				},
				{
					"gradient": "linear-gradient(0deg, #f9f9f9 0%, #0066ff 30%, #000f26 50%, #ffffff 50%, #ff7400 80%, #662e00 100%)",
					"name": "Sunset gradient",
					"slug": "sunsetgradient"
				},
				{
					"gradient": "linear-gradient(0deg, #ffffff 2%, #efccfc 7%, #de00ff 20%, #5400c1 50%, #e8f9ff 51%, #00d4ff 66%, #0086d6 80%, #0065a2 93%)",
					"name": "Chrome gradient",
					"slug": "chromegradient"
				},
				{
					"gradient": "repeating-linear-gradient(135deg, #00000008 0, #00000008 2px, transparent 2px, transparent 4px), linear-gradient(45deg, #83bbf7, #4a95fc, #3b68f7)",
					"name": "Custom gradient with the brand colors and stripes",
					"slug": "customgradient"
				}
			],
			"duotone": [
				{
					"colors": ["#101112", "#F6F6F6"],
					"name": "Monochrome",
					"slug": "duotone-1"
				},
				{
					"colors": ["#36200C", "#F3D9CB"],
					"name": "Sepia",
					"slug": "duotone-2"
				},
				{
					"colors": ["#E3DFDF", "#4F2E24"],
					"name": "Negative",
					"slug": "duotone-3"
				},
				{
					"colors": ["#205380", "#8CC9FF"],
					"name": "Brand Light",
					"slug": "duotone-4"
				},
				{
					"colors": ["#000000", "#40A5FF"],
					"name": "Brand Dark",
					"slug": "duotone-5"
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": true,
			"customSpacingSize": true,
			"margin": true,
			"padding": true,
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Natural",
					"slug": "natural",
					"shadow": "0 4px 8px -2px #000d"
				},
				{
					"name": "Soft",
					"slug": "soft",
					"shadow": "0 2px 1px #0002, 0 4px 2px #0002, 0 8px 4px #0002, 0 16px 8px #0002, 0 32px 16px #0001"
				},
				{
					"name": "Tight",
					"slug": "tight",
					"shadow": "0 2px 3px #0002, 0 2px 2px #0004"
				},
				{
					"name": "Crisp",
					"slug": "crisp",
					"shadow": "6px 6px 0 #0004"
				},
				{
					"name": "Outline",
					"slug": "outline",
					"shadow": "10px 10px 0 -3px var(--wp--preset--color--base), 10px 10px 0 0 #000"
				},
				{
					"name": "Fancy",
					"slug": "fancy",
					"shadow": "-9px -9px 0 0 var(--wp--preset--color--primary)"
				},
				{
					"name": "Steps",
					"slug": "steps",
					"shadow": "rgba(0,0,0,0.35) 3px 3px, rgba(0,0,0,0.3) 6px 6px, rgba(0,0,0,0.2) 9px 9px, rgba(0,0,0,0.1) 12px 12px, rgba(0,0,0,0.05) 16px 16px"
				}
			]
		},
		"typography": {
			"fluid": true,
			"customFontSize": true,
			"dropCap": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textColumns": true,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": true,
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "var(--wp--custom--secondary-font), system-ui, -apple-system, 'Segoe UI', Roboto, 'Liberation Sans', sans-serif",
					"name": "Secondary Font",
					"slug": "secondary-font"
				},
				{
					"fontFamily": "var(--wp--custom--primary-font), system-ui, -apple-system, 'Segoe UI', Roboto, 'Liberation Sans', sans-serif",
					"name": "System Sans",
					"slug": "system-font"
				},
				{
					"fontFamily": "Georgia, 'Droid Serif', Times, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
					"name": "System Serif",
					"slug": "system-serif"
				},
				{
					"fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, 'Roboto Mono', 'Liberation Mono', monospace",
					"name": "System Mono",
					"slug": "system-mono"
				},
				{
					"fontFamily": "'Brush Script MT', cursive",
					"name": "System Cursive",
					"slug": "system-cursive"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.8rem",
						"max": "1rem"
					},
					"size": "1rem",
					"name": "Small",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.2rem"
					},
					"size": "1.2rem",
					"name": "Medium",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "1.7rem"
					},
					"size": "1.6rem",
					"name": "Large",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.6rem"
					},
					"size": "2.3rem",
					"name": "Extra Large",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.5rem",
						"max": "3.6rem"
					},
					"size": "3.6rem",
					"name": "X-Extra Large",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"css": "code,kbd,samp,pre{font-family:var(--wp--preset--font-family--system-mono);unicode-bidi:bidi-override;white-space:pre-wrap;font-size:inherit}iframe{border:0}b,strong{font-weight:bolder}sub,sup{font-size:75%;line-height:0;position:relative}kbd{background-color:#ddd;padding:1px 4px;border-radius:5px;border:1px solid #aaa;border-bottom-width:3px;white-space:nowrap}.has-text-align-center{text-wrap:balance}a{text-decoration-thickness:1px!important;text-underline-offset:.1em}.is-root-container{max-width:var(--wp--style--global--content-size);margin:1em auto}.wp-block-gallery:not(.is-style-default) figure.wp-block-image:has(figcaption):before{backdrop-filter:none;-webkit-mask-image:none;mask-image:none}.wp-block-gallery:not(.is-style-default)",
		"color": {
			"background": "var:preset|color|base",
			"text": "var:preset|color|contrast"
		},
		"elements": {
			"h1": {
				"css": "background-color:var(--wp--preset--color--secondary);background-image:linear-gradient(0deg,#e65100 0%,#f4971c 54%,#ffe700 100%);-webkit-background-clip:text;background-clip:text;-webkit-text-fill-color:transparent;filter:drop-shadow(1px 1px 0 #444) drop-shadow(1px 1px 0px #777)",
				"typography": {
					"fontSize": "var:preset|font-size|xx-large",
					"fontWeight": "700",
					"letterSpacing": "-0.1rem"
				},
				"spacing": {
					"padding": {
						"bottom": "4px"
					}
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2.2rem",
					"fontWeight": "600"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.75rem",
					"fontWeight": "600"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.6rem",
					"fontWeight": "600"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.25rem",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1.1rem",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"lineHeight": "1.15"
				},
				"spacing": {
					"margin": {
						"bottom": "0.5rem"
					}
				}
			},
			"caption": {
				"typography": {
					"fontSize": "0.9rem",
					"lineHeight": "1.15"
				},
				"spacing": {
					"padding": {
						"top": "0.3em"
					}
				}
			},
			"cite": {
				"typography": {
					"lineHeight": "1.15"
				}
			},
			"button": {
				"css": "transition:all 0.25s ease-in-out",
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var:preset|color|primary",
					"text": "var:preset|color|black"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|primary-pale",
						"text": "var:preset|color|black"
					}
				},
				":active": {
					"color": {
						"background": "var:preset|color|primary-dark",
						"text": "var:preset|color|white"
					}
				},
				":focus": {
					"color": {
						"background": "var:preset|color|black",
						"text": "var:preset|color|white"
					}
				}
			},
			"link": {
				":link": {
					"color": {
						"text": "inherit"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"right": "var:preset|spacing|30",
				"bottom": "var:preset|spacing|40",
				"left": "var:preset|spacing|30",
				"top": "var:preset|spacing|40"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|system-font",
			"lineHeight": "1.7",
			"fontSize": "var:preset|font-size|medium"
		},
		"blocks": {
			"core/paragraph": {
				"css": "text-wrap:pretty;orphans:3;widows:3; &.has-drop-cap:not(:focus)::first-letter{font-weight:700 !important;font-size:450% !important} & code{background-color:#aaa6;padding:0 2px;border-radius:2px}",
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "1.7"
				},
				"spacing": {
					"margin": {
						"bottom": "0.75rem"
					}
				},
				"variations": {
					"text-shadow-soft": {
						"css": "text-shadow:1px 1px 3px #0003"
					},
					"text-shadow-hard": {
						"css": "text-shadow:1px 1px 1px #0008"
					},
					"text-outline": {
						"css": "text-shadow:1px 0 #000,1px 1px #000,-1px -1px #000,1px -1px #000,-1px 1px #000,1px 1px #000"
					},
					"text-justify": {
						"typography": {
							"textAlign": "justify"
						}
					},
					"text-subtitle": {
						"color": {
							"text": "#555"
						},
						"typography": {
							"fontSize": "1.5rem",
							"fontWeight": "600",
							"lineHeight": "1.1 !important",
							"letterSpacing": "-1px",
							"fontFamily": "var:preset|font-family|secondary-font"
						},
						"spacing": {
							"margin": {
								"top": "0.6em",
								"bottom": "0.6em",
								"left": "0.1em",
								"right": "0.1em"
							}
						}
					}
				}
			},
			"core/heading": {
				"css": "margin:1.2rem auto 0.5rem;text-wrap:pretty",
				"typography": {
					"lineHeight": "1"
				},
				"variations": {
					"header-shadow-soft": {
						"css": "text-shadow:1px 1px 4px #0004"
					},
					"header-shadow-hard": {
						"css": "text-shadow:2px 2px #0008"
					},
					"header-underline": {
						"css": "border-bottom:2px solid #ddd;padding-bottom:3px"
					},
					"header-title": {
						"css": "color:var(--wp--preset--color--secondary);font-weight:700;letter-spacing:-0.1rem"
					},
					"header-text-fill": {
						"css": "padding:0 !important;-webkit-background-clip:text !important;background-clip:text !important;-webkit-text-fill-color:transparent;line-height:1.1"
					},
					"header-embelish": {
