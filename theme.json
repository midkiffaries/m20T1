{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "800px",
			"wideSize": "1050px"
		},
		"color": {
			"custom": true,
			"customGradient": true,
			"link": true,
			"gradients": [],
			"palette": [
				{
					"name": "Green",
					"slug": "green",
					"color": "#bada55"
				},
				{
					"name": "Base Color Dark",
					"slug": "baseDark",
					"color": "#205380"
				},
				{
					"name": "Base Color",
					"slug": "base",
					"color": "#40A5FF"
				},
				{
					"name": "Base Color Pale",
					"slug": "basePale",
					"color": "#8CC9FF"
				}
			]
		},
		"typography": {
			"dropCap": true,
			"customFontSize": true,
            "lineHeight": true,
			"fontWeight": true,
			"letterSpacing": true,
			"fontStyle": true,
			"textDecoration": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Montserrat\",sans-serif",
					"name": "Montserrat",
					"slug": "montserrat"
				}
			],
			"fontSizes": [
				{
					"size": "1rem",
					"slug": "small"
				},
				{
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"size": "clamp(1.75rem, 3vw, 2.25rem)",
					"slug": "x-large"
				}
			]
		},
		"spacing": {
			"blockGap": true,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"custom": {
			"spacing": {
				"small": "max(1.25rem, 5vw)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "var(--wp--custom--spacing--small, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"huge": "clamp(2.25rem, 4vw, 2.75rem)",
					"gigantic": "clamp(2.75rem, 6vw, 3.25rem)",
					"colossal": "clamp(3.25rem, 8vw, 6.25rem)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.6
				}
			}
		}
	}
}